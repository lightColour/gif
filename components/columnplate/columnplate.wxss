/* components/columnplate/columnplate.wxss */
.columnplate-title{
  background-image:url("data:image/png;base64,");
  background-repeat: no-repeat;
  
}
.columnplate-container{
  border-bottom:5rpx solid blue;
}
.head{
   font-size:32rpx;
   padding:35rpx 25rpx;  
   display: flex;
   flex-wrap: wrap;
   justify-content: space-between;
}
.columnplate-title{
  text-align: center;
  display: block;
  width:202rpx;
  line-height: 44rpx;
  background-size:100%; 
}
.more{
  margin-right:20rpx;
}
.more image{
  width: 20rpx;
  height:20rpx;

}
.content{
  padding:0 40rpx;
  padding-bottom:30rpx;
   display: flex;
   flex-wrap: wrap;
   justify-content: space-between;
}

.content image{
  width:315rpx;
  border-radius: 20rpx;
  

}
.content-item .name{
   font-size:24rpx; 
   vertical-align: top;
}
