/**index.wxss**/
.index{
  background: #fafafa;
  /* display: flex;
  flex-direction: column; */
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
}
.index-top{
  display: block;
  background: #19a4ff;
  overflow: hidden;
  height:85rpx;
}
.index-search{
  color:white;
  background-color: #46b6fe;
  display: flex;
  margin:15rpx;
  align-items: center;
  height: 55rpx;
  border-radius: 55rpx;
  font-size:26rpx;
}
.index-search icon{
  margin-left:20rpx;
  margin-right:20rpx;
}
.index-search text{
  display: block;
  padding:0;
}
.index-content{
  /* flex: 1; */
  height: 90%;
}
.index-content swiper{
  height:100%;
}
/*首页头部导航*/
.index-nav{  
  font-size:28rpx;
  color:#303030;
  background: #fff;
}
.index-nav .scroll-view {
  white-space:nowrap;
}
.index-nav-item{
  position: relative;
  display: inline-block;
  padding: 28rpx 0rpx;
  text-align: center;
  width:34%;
}
.index-nav .nav-bottom-wrap{
  position: absolute;
  left:0px;
  bottom:0px;
  width:100%;
}
.index-nav-active .nav-bottom-indicator{
  width: 30%;
  height:8rpx;
  margin:0rpx auto;
  background: linear-gradient(to right,#fb8012, #fda430);
  border-radius:10rpx;
}
/*首页推荐模块*/
.rec-container,.song-container{
  height:100%;
  width:100%;
  border-top:10rpx solid #f0fafb;
}
.rec-top{
  height:277rpx;
  border-bottom:10rpx solid #f0fafb;  
}
.rec-top image{
  width:100%;  
}
.rec-top-left{
  float:left;
  width:500rpx;
  margin-right: 5rpx;
}
.rec-top-left image{
  height:100%;
}
.rec-top-left image{
  display: block;
}
.rec-top-right{
  float:right;
  width:244rpx;
  height: 277rpx;
  overflow: hidden;
  /* height: 100%; */

}
.rec-top-right view{
   height: 49%; 
   overflow: hidden;
   margin-bottom: 5rpx;
}
.rec-top-right image{
  width:100%;
}
.rec-classify{
   background: #fff; 
  display: flex;
  flex-direction: row;
  padding: 40rpx 0rpx;
  border-bottom:10rpx solid #f0fafb; 
}
.rec-classify-item{
  flex:1;
  text-align: center;
  font-size:24rpx;
  color:#666666;
}
.rec-classify-item image{
  width:120rpx;
  height:120rpx;
  border-radius:120rpx;
  display:inline-block;
}
.rec-classify-item .rec-classify-item-font{
  padding-top:15rpx;
  font-size: 28rpx;
}
/*首页推荐列表模块*/
.song-container{
  background: #fff;
}
.columnplate-container{
  border-bottom:10rpx solid #f0fafb;
  background:#fff;
}
.columnplate-container .head{
   font-size:32rpx;
    padding:26rpx 25rpx;   
   display: flex;
   flex-wrap: wrap;
   justify-content: space-between;
   align-items: center; 
}
.columnplate-container .head-title image{
  display: block;
  width:200rpx;
  height: 44rpx;
}
.columnplate-container .more{
  margin-right:20rpx;
  font-size: 28rpx;
  color:#989898;
}
.columnplate-container .more image{
  width: 20rpx;
  height:20rpx;

}
.columnplate-container .content{
  padding:0 40rpx;
  padding-bottom:30rpx;
   display: flex;
   flex-wrap: wrap;
   justify-content: space-between;

}

.columnplate-container .content image{
  width:315rpx;
  border-radius: 20rpx;
  display:block;
  height: 196rpx;
}
.columnplate-container .content-item .name{
   margin-top:15rpx;
   display: inline-block;
   font-size:28rpx; 
   vertical-align: top;
   color:#676767; 
   margin-bottom:40rpx;
   margin-left:10rpx;
}
.recommand-container .head{
   height:100rpx; 
   display: flex;
   align-items: center;
   font-size:30rpx;
   text-align: center;
   padding-left:40rpx;
}
.recommand-container .head image{
  width:200rpx;
}
.recommand-container .content{
  padding:40rpx 40rpx;
  padding-top:0;
}
.recommand-container .content-item{
  display: flex;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  border-bottom:1rpx solid #ededed;
  

}
.recommand-container .content-item .left-img{
  flex:28;
}
.recommand-container .content-item .left-img image{
  width: 100%;
  border-radius: 20rpx;
  display:block;
  height:176rpx;
  
}
.recommand-container .content-item .right-font{
  flex:39;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  font-size:30rpx;
  color:#696969;
}
/*分类歌曲列表*/
.song-item{
  padding:20rpx 40rpx;
  display: flex;
  border-bottom:1rpx solid #ededed;
}
.song-item:first-child{
  padding-top: 40rpx;
}
.song-item .left-img{
  flex:28;
}
.song-item .left-img image{
  width: 100%;
  border-radius: 20rpx;
  display:block;
  height: 176rpx;
  
}
.song-item .right-font{
  padding-left:20rpx;
  flex:39;
  display: flex;
  flex-direction: column; 
  justify-content: center; 
}
.song-item .right-font .song-title text{
  display: block;
  font-size:32rpx;
}
.song-item .right-font .song-des text{
  padding-top:10rpx;
  display: block;
  font-size:26rpx;
  color:#999999;
}
.song-item .right-font .song-much text{
  padding-top:10rpx;
  display:block;
  font-size:26rpx;
  color:#999999;
}
.loading{
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: center;
  margin-bottom: 50rpx;
}
.loading-1{
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: center;
   margin-bottom: 20rpx;
}
.scroll-view{
  width: 100%;
}
.search-img{
  position: fixed;
  width:90rpx;
  height: 90rpx;
  right:27rpx;
  bottom:27rpx;  
}
.back-app-btn{
  background: url(data:image/png;base64,);
  background-size: 100%;
  position: fixed;
  background-repeat: no-repeat;
  font-size: 0;
  width:150rpx;
  height:100rpx;
  right:5rpx;
  bottom:220rpx;
  z-index: 9;
}
.back-app-btn::after{
  border:none;
}
.contact-btn{
  background: url(data:image/png;base64,);
  background-size: 100%;
  position: fixed;
  background-repeat: no-repeat;
  font-size: 0;
  width:92rpx;
  height:92rpx;
  right:26rpx;
  bottom:120rpx;
  z-index: 9;
}
.contact-btn::after{
  border:none;
}
